    "def custom_tokenizer(document):\n",
    "    import spacy \n",
    "    en_nlp = spacy.load('en_core_web_sm')\n",
    "    doc_spacy = en_nlp(document)\n",
    "    return [token.lemma_ for token in doc_spacy]\n",
